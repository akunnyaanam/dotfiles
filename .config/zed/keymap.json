[
	{
		"context": "Workspace",
		"bindings": {
			"cmd-shift-p": "command_palette::Toggle",
			"ctrl-shift-p": "command_palette::Toggle",
			"cmd-b": "workspace::ToggleLeftDock",
			"ctrl-b": "workspace::ToggleLeftDock",
			"cmd-shift-e": "project_panel::ToggleFocus",
			"ctrl-shift-e": "project_panel::ToggleFocus",
			"cmd-t": "terminal_panel::ToggleFocus",
			"ctrl-t": "terminal_panel::ToggleFocus",
			"cmd-shift-t": "task::Spawn",
			"cmd-k cmd-s": "workspace::SaveAll",
			"ctrl-k ctrl-s": "workspace::SaveAll",
		},
	},
	{
		"context": "Editor",
		"bindings": {
			"cmd-shift-i": "editor::Format",
			"ctrl-shift-i": "editor::Format",
			"cmd-d": "editor::SelectNext",
			"ctrl-d": "editor::SelectNext",
			"cmd-shift-l": "editor::SelectAllMatches",
			"ctrl-shift-l": "editor::SelectAllMatches",
			"cmd-/": "editor::ToggleComments",
			"ctrl-/": "editor::ToggleComments",
			"cmd-shift-d": "editor::DuplicateSelection",
			"ctrl-shift-d": "editor::DuplicateSelection",
			"cmd-shift-k": "editor::DeleteLine",
			"ctrl-shift-k": "editor::DeleteLine",
			"alt-up": "editor::MoveLineUp",
			"alt-down": "editor::MoveLineDown",
			"cmd-shift-f": "workspace::NewSearch",
			"ctrl-shift-f": "workspace::NewSearch",
			"cmd-p": "file_finder::Toggle",
			"ctrl-p": "file_finder::Toggle",
			"cmd-shift-o": "outline::Toggle",
			"ctrl-shift-o": "outline::Toggle",
		},
	},
	{
		"context": "Terminal",
		"bindings": {
			"cmd-t": "terminal_panel::ToggleFocus",
			"ctrl-t": "terminal_panel::ToggleFocus",
			"cmd-w": "pane::CloseActiveItem",
			"ctrl-shift-w": "pane::CloseActiveItem",
			"cmd-k": "terminal::Clear",
			"ctrl-k": "terminal::Clear",
		},
	},
	{
		"context": "ProjectPanel",
		"bindings": {
			"a": "project_panel::NewFile",
			"shift-a": "project_panel::NewDirectory",
			"r": "project_panel::Rename",
			"delete": "project_panel::Delete",
			"backspace": "project_panel::Delete",
			"cmd-c": "project_panel::Copy",
			"ctrl-c": "project_panel::Copy",
			"cmd-x": "project_panel::Cut",
			"ctrl-x": "project_panel::Cut",
			"cmd-v": "project_panel::Paste",
			"ctrl-v": "project_panel::Paste",
		},
	},
	{
		"context": "Workspace",
		"bindings": {
			"ctrl-alt-j": "workspace::ToggleBottomDock",
		},
	},
	{
		"context": "Workspace",
		"bindings": {
			"shift shift": "file_finder::Toggle",
		},
	},
	{
		"context": "Workspace",
		"bindings": {
			"shift ctrl": "outline::Toggle",
		},
	},
	{
		"context": "Workspace",
		"bindings": {
			"ctrl-#": "workspace::NewCenterTerminal",
		},
	},
	{
		"context": "Terminal",
		"bindings": {
			"ctrl-shift-k": "terminal::ScrollLineUp",
			"ctrl-shift-j": "terminal::ScrollLineDown",
		},
	},
	{
		"context": "Workspace",
		"bindings": {
			"ctrl-!": "project_panel::CollapseAllEntries",
		},
	},
	{
		"context": "Editor",
		"bindings": {
			"alt-shift-c": "editor::Copy",
		},
	},
	{
		"context": "Workspace",
		"bindings": {
			"ctrl-b": null,
			"cmd-b": null,
		},
	},
	{
		"bindings": {
			"cmd-i": "tab_switcher::Toggle",
		},
	},
	{
		"context": "Editor && VimControl && !VimWaiting && !menu",
		"bindings": {
			"space i": "tab_switcher::Toggle",
		},
	},
	{
		"context": "TabSwitcher",
		"bindings": {
			"k": "menu::SelectPrevious",
			"j": "menu::SelectNext",
			"l": "menu::Confirm",
			"x": "tab_switcher::CloseSelectedItem",

			"cmd-backspace": "tab_switcher::CloseSelectedItem",
		},
	},
	{
		"context": "Editor && VimControl && !VimWaiting && !menu",
		"bindings": {
			"space e": "project_panel::ToggleFocus",
		},
	},
	{
		"context": "Editor && VimControl && !VimWaiting && !menu",
		"bindings": {
			"space g": "git_panel::ToggleFocus",
		},
	},
	{
		"context": "Editor && VimControl && !VimWaiting && !menu",
		"bindings": {
			"space b": "outline_panel::ToggleFocus",
		},
	},
	{
		"context": "OutlinePanel",
		"bindings": {
			"h": "menu::SelectParent",
			"k": "menu::SelectPrevious",
			"j": "menu::SelectNext",
			"l": "menu::Confirm",

			"cmd-backspace": "tab_switcher::CloseSelectedItem",
		},
	},
	{
		"context": "Editor && VimControl && !VimWaiting && !menu",
		"bindings": {
			"space t": "terminal_panel::ToggleFocus",
		},
	},
	{
		"context": "Editor && VimControl && !VimWaiting && !menu",
		"bindings": {
			"space f": "workspace::ToggleRightDock",
		},
	},
	{
		"context": "ProjectPanel && not_editing",
		"bindings": {
			// https://github.com/zed-industries/zed/issues/4753
			// https://github.com/zed-industries/zed/issues/4270
			"escape": "project_panel::ToggleFocus",
			"enter": "project_panel::Open",
			"o": "project_panel::NewDirectory",
			"a": "project_panel::NewFile",
			"r": "project_panel::Rename",
			"x": "project_panel::Cut",
			"y": "project_panel::Copy",
			"p": "project_panel::Paste",
			"d": "project_panel::Delete",
			// Directory expansion
			"l": "project_panel::ExpandSelectedEntry",
			"h": "project_panel::CollapseSelectedEntry",
			// Move up and down
			"j": "menu::SelectNext",
			"k": "menu::SelectPrevious",
			// "shift-d": "project_panel::Delete"
		},
	},
	{
		"context": "ProjectPanel",
		"bindings": {
			"r": null,
		},
	},
]
